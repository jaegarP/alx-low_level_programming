#include <stdio.h>
#include <stdlib.h>
#include "main.h"

/**
 * main - prints multiplication of argumnts
 * @argc: number of args
 * @argv: array of args
 *
 * Return: 0
 */

int main(int argc, char *argv[])
{
	/*int *op1;
	  int *op2;*/
	char *op1 = argv[1];
	char *op2 = argv[2];

	if (argc != 3)
	{
		printf ("Error\n");
		return (1);
	}
	printf ("%s\n", argv[0]);
	/*printf ("%p\n", (void*)op1);*/
	printf ("%d\n", *op1);
	printf ("%d\n", *op2);
	printf ("%d\n", atoi(argv[1]) * atoi(argv[2]));
	return (0);
}
