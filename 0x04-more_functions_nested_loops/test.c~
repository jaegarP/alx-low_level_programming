#include "main.h"

/**
 * fizzbuzz - prints fizzbuzz based on multiple
 */
void fizz_buzz()
{
	int i;
	char p;
	for (i = 1; i <= 100; i++)
	{
		if (i % 3 == 0)
		{
			p = "Fizz"
		}
		else if (i % 5 == 0)
		{
			p = "Buzz"
		}
		else if (i % 3 == 0 && i % 5 == 0)
		{
			p = "FizzBuzz"
		}
		else
		{
			p = i + '0'
		}
		printf("%c ", p)
	}
}
